{
  "name": "mussia4",
  "version": "1.0.0",
  "description": "Mussia project with typescript",
  "main": "index.js",
  "scripts": {
    "artifactregistry-login": "npx google-artifactregistry-auth .npmrc",
    "bootstrap": "lerna bootstrap -- --production --no-optional",
    "postinstall": "lerna exec --parallel -- npm i",
    "prepare:ejs": "lerna run build:client",
    "fucntion": "lerna run fucntion",
    "start": "lerna run --parallel start",
    "prebuild": "lerna exec -- rimraf dist functions",
    "build": "lerna run --parallel build",
    "lint": "lerna run --parallel lint",
    "build:image": "lerna run --parallel build:image",
    "test": "lerna run --stream test",
    "npm-publish": "bash scripts/npm-publish.sh",
    "compose:dev:up": "docker-compose -f docker-compose.dev.yaml up -d",
    "compose:dev:down": "docker-compose -f docker-compose.dev.yaml down",
    "compose:prod:up": "docker-compose up -d",
    "compose:prod:build": "docker-compose build",
    "compose:prod:down": "docker-compose down",
    "docker:local": "lerna run --parallel docker:local",
    "e2e": "lerna run --stream e2e",
    "deploy:docker:single-host": "bash scripts/deployments/local-docker-swarm.sh",
    "deploy": "lerna run --parallel deploy",
    "link": "lerna run --stream link",
    "unlink": "lerna run --stream unlink",
    "tsc": "lerna run --stream tsc",
    "pre-push": "node scripts/hooks/pre-push.js",
    "pre-commit": "node scripts/hooks/pre-commit.js",
    "prepare-commit-msg": "node scripts/hooks/prepare-commit-msg.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yurikrupnik/mussia4.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/yurikrupnik/mussia4/issues"
  },
  "homepage": "https://github.com/yurikrupnik/mussia4#readme",
  "devDependencies": {
    "@loadable/webpack-plugin": "^5.14.0",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.10",
    "@types/react": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.8.2",
    "@typescript-eslint/parser": "^4.8.2",
    "autoprefixer": "^10.0.4",
    "css-hot-loader": "^1.4.4",
    "css-loader": "^5.0.1",
    "esbuild-loader": "^2.4.0",
    "eslint": "^7.14.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "file-loader": "^6.2.0",
    "jest": "^26.6.3",
    "lerna": "^3.22.1",
    "lodash": "^4.17.20",
    "mini-css-extract-plugin": "^1.3.1",
    "prettier": "^2.2.1",
    "raw-loader": "^4.0.2",
    "rollup": "^2.33.3",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-esbuild": "^2.6.0",
    "rollup-plugin-postcss": "^3.1.8",
    "rollup-plugin-typescript2": "^0.29.0",
    "sass-loader": "^10.1.0",
    "style-loader": "^2.0.0",
    "ts-loader": "^8.0.11",
    "typescript": "^4.1.2",
    "webpack": "^5.8.0"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|less|scss)$": "identity-obj-proxy"
    }
  },
  "prettier": {
    "arrowParens": "always",
    "printWidth": 120,
    "tabWidth": 4
  }
}
