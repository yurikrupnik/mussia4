    "test": "jest src --colors",
    "build": "rollup --config=../../../rollup.config.functions.ts",
    "predeploy": "npm run build",
    "deploy": "bash ../../../scripts/deploy-to-cloud-functions.sh"
  },
  "devDependencies": {
    "@google-cloud/functions-framework": "^1.3.2",
